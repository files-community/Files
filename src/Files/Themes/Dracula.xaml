<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:BelowWindows10version1809="http://schemas.microsoft.com/winfx/2006/xaml/presentation?IsApiContractNotPresent(Windows.Foundation.UniversalApiContract, 7)"
    xmlns:Windows10version1809="http://schemas.microsoft.com/winfx/2006/xaml/presentation?IsApiContractPresent(Windows.Foundation.UniversalApiContract, 7)">
    <ResourceDictionary.ThemeDictionaries>
        <ResourceDictionary x:Key="Default">
            <!--  Background Resources  -->
            <SolidColorBrush x:Key="RootBackgroundBrush" Color="#AA292A36" />
            <Color x:Key="SolidBackgroundFillColorBase">#434456</Color>
            <Color x:Key="SolidBackgroundFillColorSecondary">#292A36</Color>
            <Color x:Key="SolidBackgroundFillColorTertiary">#292A36</Color>
            <Color x:Key="SolidBackgroundFillColorQuarternary">#46475B</Color>
            <SolidColorBrush x:Key="SolidBackgroundFillColorBaseBrush" Color="{ThemeResource SolidBackgroundFillColorBase}" />
            <SolidColorBrush x:Key="SolidBackgroundFillColorSecondaryBrush" Color="{ThemeResource SolidBackgroundFillColorSecondary}" />
            <SolidColorBrush x:Key="SolidBackgroundFillColorTertiaryBrush" Color="{ThemeResource SolidBackgroundFillColorTertiary}" />
            <SolidColorBrush x:Key="SolidBackgroundFillColorQuarternaryBrush" Color="{ThemeResource SolidBackgroundFillColorQuarternary}" />
            <!--  Acrylic Resources  -->
            <Color x:Key="SolidBackgroundAcrylic">#46475B</Color>
            <!--  Accent Color  -->
            <Color x:Key="SystemAccentColor">#BABCF3</Color>
            <Color x:Key="SystemAccentColorDark1">#BABCF3</Color>
            <Color x:Key="SystemAccentColorDark2">#BABCF3</Color>
            <Color x:Key="SystemAccentColorDark3">#BABCF3</Color>
            <Color x:Key="SystemAccentColorLight1">#BABCF3</Color>
            <Color x:Key="SystemAccentColorLight2">#BABCF3</Color>
            <Color x:Key="SystemAccentColorLight3">#BABCF3</Color>
        </ResourceDictionary>
        <ResourceDictionary x:Key="Light">
            <!--  Background Resources  -->
            <SolidColorBrush x:Key="RootBackgroundBrush" Color="#AA8082A7" />
            <Color x:Key="SolidBackgroundFillColorBase">#9B9EC8</Color>
            <Color x:Key="SolidBackgroundFillColorSecondary">#8082A7</Color>
            <Color x:Key="SolidBackgroundFillColorTertiary">#8082A7</Color>
            <Color x:Key="SolidBackgroundFillColorQuarternary">#686989</Color>
            <SolidColorBrush x:Key="SolidBackgroundFillColorBaseBrush" Color="{ThemeResource SolidBackgroundFillColorBase}" />
            <SolidColorBrush x:Key="SolidBackgroundFillColorSecondaryBrush" Color="{ThemeResource SolidBackgroundFillColorSecondary}" />
            <SolidColorBrush x:Key="SolidBackgroundFillColorTertiaryBrush" Color="{ThemeResource SolidBackgroundFillColorTertiary}" />
            <SolidColorBrush x:Key="SolidBackgroundFillColorQuarternaryBrush" Color="{ThemeResource SolidBackgroundFillColorQuarternary}" />
            <!--  Acrylic Resources  -->
            <Color x:Key="SolidBackgroundAcrylic">#686989</Color>
            <!--  Accent Color  -->
            <Color x:Key="SystemAccentColor">#282935</Color>
            <Color x:Key="SystemAccentColorDark1">#282935</Color>
            <Color x:Key="SystemAccentColorDark2">#282935</Color>
            <Color x:Key="SystemAccentColorDark3">#282935</Color>
            <Color x:Key="SystemAccentColorLight1">#282935</Color>
            <Color x:Key="SystemAccentColorLight2">#282935</Color>
            <Color x:Key="SystemAccentColorLight3">#282935</Color>
        </ResourceDictionary>
    </ResourceDictionary.ThemeDictionaries>
</ResourceDictionary>
