<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:BelowWindows10version1809="http://schemas.microsoft.com/winfx/2006/xaml/presentation?IsApiContractNotPresent(Windows.Foundation.UniversalApiContract, 7)"
    xmlns:Windows10version1809="http://schemas.microsoft.com/winfx/2006/xaml/presentation?IsApiContractPresent(Windows.Foundation.UniversalApiContract, 7)">
    <ResourceDictionary.ThemeDictionaries>
        <ResourceDictionary x:Key="Default">
            <!--  Background Resources  -->
            <SolidColorBrush x:Key="RootBackgroundBrush" Color="#AA1A2B46" />
            <Color x:Key="SolidBackgroundFillColorBase">#233F6C</Color>
            <Color x:Key="SolidBackgroundFillColorSecondary">#1A2B46</Color>
            <Color x:Key="SolidBackgroundFillColorTertiary">#213B65</Color>
            <Color x:Key="SolidBackgroundFillColorQuarternary">#274472</Color>
            <SolidColorBrush x:Key="SolidBackgroundFillColorBaseBrush" Color="{ThemeResource SolidBackgroundFillColorBase}" />
            <SolidColorBrush x:Key="SolidBackgroundFillColorSecondaryBrush" Color="{ThemeResource SolidBackgroundFillColorSecondary}" />
            <SolidColorBrush x:Key="SolidBackgroundFillColorTertiaryBrush" Color="{ThemeResource SolidBackgroundFillColorTertiary}" />
            <SolidColorBrush x:Key="SolidBackgroundFillColorQuarternaryBrush" Color="{ThemeResource SolidBackgroundFillColorQuarternary}" />
            <!--  Acrylic Resources  -->
            <Color x:Key="SolidBackgroundAcrylic">#274472</Color>
            <!--  Accent Color  -->
            <Color x:Key="SystemAccentColor">#3967B1</Color>
            <Color x:Key="SystemAccentColorDark1">#3967B1</Color>
            <Color x:Key="SystemAccentColorDark2">#3967B1</Color>
            <Color x:Key="SystemAccentColorDark3">#3967B1</Color>
            <Color x:Key="SystemAccentColorLight1">#3967B1</Color>
            <Color x:Key="SystemAccentColorLight2">#3967B1</Color>
            <Color x:Key="SystemAccentColorLight3">#3967B1</Color>
        </ResourceDictionary>
        <ResourceDictionary x:Key="Light">
            <!--  Background Resources  -->
            <SolidColorBrush x:Key="RootBackgroundBrush" Color="#AA4170BE" />
            <Color x:Key="SolidBackgroundFillColorBase">#4781DE</Color>
            <Color x:Key="SolidBackgroundFillColorSecondary">#4170BE</Color>
            <Color x:Key="SolidBackgroundFillColorTertiary">#4E87E4</Color>
            <Color x:Key="SolidBackgroundFillColorQuarternary">#4781DE</Color>
            <SolidColorBrush x:Key="SolidBackgroundFillColorBaseBrush" Color="{ThemeResource SolidBackgroundFillColorBase}" />
            <SolidColorBrush x:Key="SolidBackgroundFillColorSecondaryBrush" Color="{ThemeResource SolidBackgroundFillColorSecondary}" />
            <SolidColorBrush x:Key="SolidBackgroundFillColorTertiaryBrush" Color="{ThemeResource SolidBackgroundFillColorTertiary}" />
            <SolidColorBrush x:Key="SolidBackgroundFillColorQuarternaryBrush" Color="{ThemeResource SolidBackgroundFillColorQuarternary}" />
            <!--  Acrylic Resources  -->
            <Color x:Key="SolidBackgroundAcrylic">#4781DE</Color>
            <!--  Accent Color  -->
            <Color x:Key="SystemAccentColor">#82B0FE</Color>
            <Color x:Key="SystemAccentColorDark1">#82B0FE</Color>
            <Color x:Key="SystemAccentColorDark2">#82B0FE</Color>
            <Color x:Key="SystemAccentColorDark3">#82B0FE</Color>
            <Color x:Key="SystemAccentColorLight1">#82B0FE</Color>
            <Color x:Key="SystemAccentColorLight2">#82B0FE</Color>
            <Color x:Key="SystemAccentColorLight3">#82B0FE</Color>
        </ResourceDictionary>
    </ResourceDictionary.ThemeDictionaries>
</ResourceDictionary>
