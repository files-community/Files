<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:BelowWindows10version1809="http://schemas.microsoft.com/winfx/2006/xaml/presentation?IsApiContractNotPresent(Windows.Foundation.UniversalApiContract, 7)"
	xmlns:Windows10version1809="http://schemas.microsoft.com/winfx/2006/xaml/presentation?IsApiContractPresent(Windows.Foundation.UniversalApiContract, 7)">
	<ResourceDictionary.ThemeDictionaries>
		<ResourceDictionary x:Key="Default">
			<!--  Background Resources  -->
			<SolidColorBrush x:Key="RootBackgroundBrush" Color="#AA6D262F" />
			<Color x:Key="SolidBackgroundFillColorBase">#912f3c</Color>
			<Color x:Key="SolidBackgroundFillColorSecondary">#6D262F</Color>
			<Color x:Key="SolidBackgroundFillColorTertiary">#882F3B</Color>
			<Color x:Key="SolidBackgroundFillColorQuarternary">#882F3B</Color>
			<SolidColorBrush x:Key="SolidBackgroundFillColorBaseBrush" Color="{ThemeResource SolidBackgroundFillColorBase}" />
			<SolidColorBrush x:Key="SolidBackgroundFillColorSecondaryBrush" Color="{ThemeResource SolidBackgroundFillColorSecondary}" />
			<SolidColorBrush x:Key="SolidBackgroundFillColorTertiaryBrush" Color="{ThemeResource SolidBackgroundFillColorTertiary}" />
			<SolidColorBrush x:Key="SolidBackgroundFillColorQuarternaryBrush" Color="{ThemeResource SolidBackgroundFillColorQuarternary}" />
			<!--  Accent Color  -->
			<Color x:Key="SystemAccentColor">#D4495C</Color>
			<Color x:Key="SystemAccentColorDark1">#D4495C</Color>
			<Color x:Key="SystemAccentColorDark2">#D4495C</Color>
			<Color x:Key="SystemAccentColorDark3">#D4495C</Color>
			<Color x:Key="SystemAccentColorLight1">#D4495C</Color>
			<Color x:Key="SystemAccentColorLight2">#D4495C</Color>
			<Color x:Key="SystemAccentColorLight3">#D4495C</Color>
		</ResourceDictionary>
		<ResourceDictionary x:Key="Light">
			<!--  Background Resources  -->
			<SolidColorBrush x:Key="RootBackgroundBrush" Color="#AA93333F" />
			<Color x:Key="SolidBackgroundFillColorBase">#A33543</Color>
			<Color x:Key="SolidBackgroundFillColorSecondary">#93333F</Color>
			<Color x:Key="SolidBackgroundFillColorTertiary">#AE3C4B</Color>
			<Color x:Key="SolidBackgroundFillColorQuarternary">#AE3C4B</Color>
			<SolidColorBrush x:Key="SolidBackgroundFillColorBaseBrush" Color="{ThemeResource SolidBackgroundFillColorBase}" />
			<SolidColorBrush x:Key="SolidBackgroundFillColorSecondaryBrush" Color="{ThemeResource SolidBackgroundFillColorSecondary}" />
			<SolidColorBrush x:Key="SolidBackgroundFillColorTertiaryBrush" Color="{ThemeResource SolidBackgroundFillColorTertiary}" />
			<SolidColorBrush x:Key="SolidBackgroundFillColorQuarternaryBrush" Color="{ThemeResource SolidBackgroundFillColorQuarternary}" />
			<!--  Accent Color  -->
			<Color x:Key="SystemAccentColor">#3B1418</Color>
			<Color x:Key="SystemAccentColorDark1">#3B1418</Color>
			<Color x:Key="SystemAccentColorDark2">#3B1418</Color>
			<Color x:Key="SystemAccentColorDark3">#3B1418</Color>
			<Color x:Key="SystemAccentColorLight1">#3B1418</Color>
			<Color x:Key="SystemAccentColorLight2">#3B1418</Color>
			<Color x:Key="SystemAccentColorLight3">#3B1418</Color>
		</ResourceDictionary>
	</ResourceDictionary.ThemeDictionaries>
</ResourceDictionary>
