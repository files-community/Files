<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:BelowWindows10version1809="http://schemas.microsoft.com/winfx/2006/xaml/presentation?IsApiContractNotPresent(Windows.Foundation.UniversalApiContract, 7)"
    xmlns:Windows10version1809="http://schemas.microsoft.com/winfx/2006/xaml/presentation?IsApiContractPresent(Windows.Foundation.UniversalApiContract, 7)">
    <ResourceDictionary.ThemeDictionaries>
        <ResourceDictionary x:Key="Default">
            <!--  Background Resources  -->
            <SolidColorBrush x:Key="RootBackgroundBrush" Color="#AA2A847D" />
            <Color x:Key="SolidBackgroundFillColorBase">#2C5653</Color>
            <Color x:Key="SolidBackgroundFillColorSecondary">#2A847D</Color>
            <Color x:Key="SolidBackgroundFillColorTertiary">#27635E</Color>
            <Color x:Key="SolidBackgroundFillColorQuarternary">#2C5653</Color>
            <SolidColorBrush x:Key="SolidBackgroundFillColorBaseBrush" Color="{ThemeResource SolidBackgroundFillColorBase}" />
            <SolidColorBrush x:Key="SolidBackgroundFillColorSecondaryBrush" Color="{ThemeResource SolidBackgroundFillColorSecondary}" />
            <SolidColorBrush x:Key="SolidBackgroundFillColorTertiaryBrush" Color="{ThemeResource SolidBackgroundFillColorTertiary}" />
            <SolidColorBrush x:Key="SolidBackgroundFillColorQuarternaryBrush" Color="{ThemeResource SolidBackgroundFillColorQuarternary}" />
            <!--  Acrylic Resources  -->
            <Color x:Key="SolidBackgroundAcrylic">#26A594</Color>
            <!--  Accent Color  -->
            <Color x:Key="SystemAccentColor">#26A594</Color>
            <Color x:Key="SystemAccentColorDark1">#26A594</Color>
            <Color x:Key="SystemAccentColorDark2">#26A594</Color>
            <Color x:Key="SystemAccentColorDark3">#26A594</Color>
            <Color x:Key="SystemAccentColorLight1">#26A594</Color>
            <Color x:Key="SystemAccentColorLight2">#26A594</Color>
            <Color x:Key="SystemAccentColorLight3">#26A594</Color>
        </ResourceDictionary>
        <ResourceDictionary x:Key="Light">
            <!--  Background Resources  -->
            <SolidColorBrush x:Key="RootBackgroundBrush" Color="#AA29E7CD" />
            <Color x:Key="SolidBackgroundFillColorBase">#29E7CD</Color>
            <Color x:Key="SolidBackgroundFillColorSecondary">#29E7CD</Color>
            <Color x:Key="SolidBackgroundFillColorTertiary">#B4F6EE</Color>
            <Color x:Key="SolidBackgroundFillColorQuarternary">#29E7CD</Color>
            <SolidColorBrush x:Key="SolidBackgroundFillColorBaseBrush" Color="{ThemeResource SolidBackgroundFillColorBase}" />
            <SolidColorBrush x:Key="SolidBackgroundFillColorSecondaryBrush" Color="{ThemeResource SolidBackgroundFillColorSecondary}" />
            <SolidColorBrush x:Key="SolidBackgroundFillColorTertiaryBrush" Color="{ThemeResource SolidBackgroundFillColorTertiary}" />
            <SolidColorBrush x:Key="SolidBackgroundFillColorQuarternaryBrush" Color="{ThemeResource SolidBackgroundFillColorQuarternary}" />
            <!--  Acrylic Resources  -->
            <Color x:Key="SolidBackgroundAcrylic">#3C837A</Color>
            <!--  Accent Color  -->
            <Color x:Key="SystemAccentColor">#3C837A</Color>
            <Color x:Key="SystemAccentColorDark1">#3C837A</Color>
            <Color x:Key="SystemAccentColorDark2">#3C837A</Color>
            <Color x:Key="SystemAccentColorDark3">#3C837A</Color>
            <Color x:Key="SystemAccentColorLight1">#3C837A</Color>
            <Color x:Key="SystemAccentColorLight2">#3C837A</Color>
            <Color x:Key="SystemAccentColorLight3">#3C837A</Color>
        </ResourceDictionary>
    </ResourceDictionary.ThemeDictionaries>
</ResourceDictionary>
